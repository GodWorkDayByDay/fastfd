QPushButton[outer_stylesheet="pushbutton_pink"]:pressed{background:#f7c76a;}

QLabel[outer_stylesheet="pushbutton_yellow"],
QPushButton[outer_stylesheet="pushbutton_yellow"]{border-radius:5px;background:#f7c76a;font:16pt;border:0px;color:brown;}
QPushButton[outer_stylesheet="pushbutton_yellow"]:pressed{border:2px solid red;}

QPushButton[outer_stylesheet="pushbutton_purple"]{border-radius:5px; border:2px solid #dadadb;background:#f3f5f5; font:14pt;color:#7b7878;}
QPushButton[outer_stylesheet="pushbutton_purple"]:pressed{background:#f7c76a;color:red;}

QPushButton[outer_stylesheet="pushbutton_bottom"]{background-color: qlineargradient(spread:pad, x1:1, y1:0, x2:1, y2:1, stop :0 white, stop :0.02 white, stop:0.02 rgb(69, 69, 69), stop:1  rgba(34, 38, 39));border-radius:2px;
color: rgb(255, 255, 255);
border:0px;
}
QPushButton[outer_stylesheet="pushbutton_bottom"]:pressed{background:#dbe3e9;color:#7b7878;}


*[outer_stylesheet="normal_view_color"]{background-color: rgb(34, 38, 39);}

QFrame[outer_stylesheet="frame_detail_show"]{
